  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6272A4"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#8AFF80"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#9580FF"
      }
    },
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#FF80BF"
      }
    },
    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "#FF80BF"
      }
    },
    {
      "scope": ["storage"],
      "settings": {
        "foreground": "#FF80BF"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#8BE9FD"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#FFB86C"
      }
    }
  ]
}
