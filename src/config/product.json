  },
  "documentationUrl": "https://github.com/sparc-ide/sparc-ide/tree/main/docs",
  "releaseNotesUrl": "https://github.com/sparc-ide/sparc-ide/releases",
  "keyboardShortcutsUrlMac": "https://github.com/sparc-ide/sparc-ide/blob/main/docs/keybindings-mac.md",
  "keyboardShortcutsUrlLinux": "https://github.com/sparc-ide/sparc-ide/blob/main/docs/keybindings-linux.md",
