[
  {
    "key": "ctrl+shift+a",
    "command": "roo-code.chat",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+i",
    "command": "roo-code.insertCode",
    "when": "editorTextFocus && roo-code.hasCodeSuggestion"
  },
  {
    "key": "ctrl+shift+e",
    "command": "roo-code.explainCode",
