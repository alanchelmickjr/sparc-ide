    "command": "roo-code.refactorCode",
    "when": "editorTextFocus && editorHasSelection"
  },
  {
    "key": "ctrl+shift+d",
    "command": "roo-code.documentCode",
    "when": "editorTextFocus && editorHasSelection"
  },
  {
    "key": "ctrl+shift+t",
    "command": "roo-code.generateTests",
    "when": "editorTextFocus && editorHasSelection"
  },
  {
